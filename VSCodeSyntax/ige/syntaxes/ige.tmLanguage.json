{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ImageGameEngine",
	"patterns": [
		{
			"include": "#instructions"
		},
		{
			"include": "#parameters"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#unused"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#hexNumbers"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#functions"
		}
	],
	"repository": {
		"instructions": {
			"patterns": [
				{
					"name": "entity.name.function.ige",
					"match": "\\b(?<=^\\s*|[^\\|])(50|40|A0|A1|B0|C0|CA|D0|1A|1B|BB|EF|EE|2A|2B|2C|2D|2E|3A|3B|3C)\\s(?=([a-fA-f0-9]{2}[^\\n][a-fA-f0-9]{2})|L:)\\b"
				}
			]
		},
		"parameters": {
			"patterns": [
				{
					"name":"variable.parameter.double_param.ige",
					"match":"(?<=(?<=^\\s*|[^\\|])(40|A1|B0|C0|CA|D0|1A|1B|EF|2A|2B|2C|2D|2E|3A|3B|3C)\\s)[A-Fa-f0-9]{2}\\s[A-Fa-f0-9]{2}"
				},
				{
					"name":"variable.parameter.single_param.ige",
					"match":"(?<=(?<=^\\s*|[^\\|])(50|A0)\\s)[A-Fa-f0-9]{2}"
				},
				{
					
				}
			]
		},
		"unused": {
			"patterns": [
				{
					"name":"comment.line.ige",
					"match":"(?<=(?<=^\\s*|[^\\|])(50|A0)\\s([A-Fa-f0-9]{2}))\\s[A-Fa-f0-9]{2}"
				},
				{
					"name":"comment.line.ige",
					"match":"(?<=(?<=^\\s*|[^\\|])(BB|EE)\\s)[A-Fa-f0-9]{2}\\s[A-Fa-f0-9]{2}"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.ige",
					"match": "\\b(TO|FILL|PASS|IMPORT|LABEL|INIT_RANDOM|L:)\\b"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.line.ige",
					"match": "(#.*)"
				}
			]
		},
		"hexNumbers": {
			"patterns": [
				{
					"name": "constant.numeric.hex.ige",
					"match": "\\b[A-Fa-f0-9]{2}\\b"
				},
				{
					"name": "constant.numeric.decimal.ige",
					"match": "\\$\\d+"
				}
			]
		},
		"functions": {
			"patterns": [
				{
					"name": "string.other.ige",
					"match": "(?<=\\bL:)[A-Za-z_][A-Za-z0-9_]*|(?<=\\bLABEL\\|)[A-Za-z_][A-Za-z0-9_]*"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "variable.language.ige",
					"match": "(\\sX|\\sY)(\\+|-)?"
				},
				{
					"name":"variable.language.ige",
					"match": "\\bFF AE\\b"
				}
			]
		}
	},
	"scopeName": "source.ige"
}